<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeHelloWorld"
        tools:targetApi="31">
        <activity
            android:name=".NetworkImageActivity"
            android:exported="false"
            android:label="@string/title_activity_network_image"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".BoxWithConstraintsActivity"
            android:exported="false"
            android:label="@string/title_activity_box_with_constraints"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".ColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_column"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".RowActivity"
            android:exported="false"
            android:label="@string/title_activity_row"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".BoxActivity"
            android:exported="false"
            android:label="@string/title_activity_box"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".SurfaceActivity"
            android:exported="false"
            android:label="@string/title_activity_surface"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".ModifierActivity"
            android:exported="false"
            android:label="@string/title_activity_modifier"
            android:theme="@style/Theme.ComposeHelloWorld" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeHelloWorld">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>